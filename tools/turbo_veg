#!/bin/csh
# run-script generated by Most Wed Oct 31 10:27:58 2007
set EXP=V42    # Name your experiment here
unset noclobber
if ($1 != "") cd $1
#rm -f plasim_restart
rm -f Abort_Message
set YEAR=10
set YEARS=53
while ($YEAR < $YEARS)
   set YEAR = `expr $YEAR + 1`
   set DATANAME=`printf '%s.%03d' $EXP $YEAR`
   set DIAGNAME=`printf '%s_DIAG.%03d' $EXP $YEAR`
   set RESTNAME=`printf '%s_REST.%03d' $EXP $YEAR`

    if ($YEAR < 51 ) cp ln001 land_namelist
    if ($YEAR < 40 ) cp ln002 land_namelist
    if ($YEAR < 28 ) cp ln005 land_namelist
    if ($YEAR < 23 ) cp ln010 land_namelist
    if ($YEAR < 18 ) cp ln020 land_namelist
    if ($YEAR < 13 ) cp ln001 land_namelist

   mpirun -np 2 most_plasim_t21_l10_p2.x >${DIAGNAME}
   if (-e Abort_Message) exit 1
   if (-e puma_output) mv puma_output $DATANAME
   cp plasim_status plasim_restart
   mv plasim_status $RESTNAME
end
