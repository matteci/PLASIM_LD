cont1.f:            hordiv=utot(il,j,k+1)*delta(il,j,k+1)*dphi-utot(i,j,k+1)    &
cont1.f:     &            *delta(i,j,k+1)*dphi+vtot(i,j+1,k+1)*delta(i,j+1,k+1) &
cont1.f:     &            *dl(j+1)-vtot(il,j-1,k+1)*delta(il,j-1,k+1)*dl(j-1)
diva.f:            ubold(i,j)=ubold(i,j)+utot(i,j,k)*delta(i,j,k)
diva.f:            vbold(i,j)=vbold(i,j)+vtot(i,j,k)*delta(i,j,k)
